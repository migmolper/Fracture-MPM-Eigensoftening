
@entry{domain,
 name={\ensuremath{\varOmega}},
 description={Continuum domain}
}

@entry{contour,
 name={\ensuremath{\partial\varOmega}},
 description={Boundary of the continuum domain.}
}

@entry{dirichlet-boundary,
 name={\ensuremath{\Gamma_d}},
 description={Essential or Dirichlet boundary conditions over the contour.}
}

@entry{neumann-boundary,
{
  name={\ensuremath{\Gamma_n}},
  description={Natural or Nemann boundary conditions over the contour}
}

@entry{xi,
{
  name={\ensuremath{\vect{\xi}}},
  description={First order tensor which describes the local coordinates field}
}

@entry{stress,
{
  name={\ensuremath{\tens{\sigma}}},
  description={Second order tensor which means the Cauchy stress tensor}
}

@entry{strain,
{
  name={\ensuremath{\tens{\varepsilon}}},
  description={Second order tensor which means the Cauchy strain tensor}
}

@entry{LME-beta,
{
  name={\ensuremath{\beta}},
  description={Regularization or thermalization parameter of the Pareto set}
}

@entry{rho,
{
  name={\ensuremath{\rho}},
  description={Describes the scalar density field}
}

@entry{nu,
{
  name={\ensuremath{\nu}},
  description={Poisson ratio}
}

@entry{gamma-npc,
{
  name={\ensuremath{\gamma}},
  description={Time integration parameter for the NPC algorithm}
}

@entry{gamma-lme,
{
  name=\ensuremath{\widehat{\gamma}}},
  description={Dimensionless parameter to control the value of the
    thermalization parameter of the LME shape functions.}
}